{
  "name": "another-pythontutor-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "express": "4.17.3"
  },
  "scripts": {
    "watch-backend": "nodemon --ignore 'frontend/*' -e js index.js",
    "watch-frontend": "cd frontend/ && npx webpack --watch --progress --config webpack.config.dev.js",
    "start-dev": "concurrently \"npm:watch*\"",
    "build-prod": "cd frontend/ && npm ci && npx webpack --config webpack.config.prod.js",
    "deps": "npm install && git submodule update --init --remote",
    "deps-prod": "npm ci && git submodule update --init --remote"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16"
  }
}